{{#contentFor "topLeft"}}
    <img src="/images/player-control-prevtrack.png" class="music-control music-control-prevtrack">
    <img src="/images/player-control-playpause.png" class="music-control music-control-playpause">
    <img src="/images/player-control-nexttrack.png" class="music-control music-control-nexttrack">
{{/contentFor}}

{{#contentFor "topRight"}}
    <img src="/images/icon.png" style=" margin: 6px 15px 0 6px; float: left;">

    <div class="music-ratings" style="display: none;">
        <img class="music-control" src="/images/star-off.png">
        <img class="music-control" src="/images/star-off.png">
        <img class="music-control" src="/images/star-off.png">
        <img class="music-control" src="/images/star-off.png">
        <img class="music-control" src="/images/star-off.png">
    </div>

    <span class="music-track-nowplaying">Now playing:</span>
    <span style="text-transform: capitalize;" class="music-track-title"></span><br>

    <img src="/images/icon-favorite-off.png" class="music-control music-control-favorite-player" style="margin-left: 3px;"> Add to favorites
    <img src="/images/icon-comment.png" class="music-control music-control-comment" style="margin-left: 3px;"> Send Comment
    <!-- <span class="icon-music"></span> <span class="music-type">Other</span> -->
    <br>

    <img src="/images/icon-time.png"> <span class="music-track-length">00:00:00</span>
    <img src="/images/icon-stems.png"> Stems: <span class="music-track-stems"></span>
    <img src="/images/icon-tempo.png"> <span class="music-track-tempo"></span>
    <img src="/images/icon-mood.png"> <span class="music-track-mood"></span>
{{/contentFor}}


{{#contentFor "bottomLeft"}}
    <img src="/images/control-sting.png" class="music-control music-control-sting" style="margin: 6px 9px 1px 7px;">
    <img src="/images/control-addtopl.png" class="music-control music-control-addtopl" style="margin: 6px 0 0 0;">
    <img id="music_download" src="/images/control-download.png" class="music-control music-control-download" style="margin: 6px 9px 0px 7px;">
    <!-- <img src="/images/control-comment.png" class="music-control music-control-comments" style="margin: 6px 0 0 0;">-->
    <img src="/images/control-viewtrack.png" class="music-control music-control-stemlist" style="margin: 6px 0 0 0;">
{{/contentFor}}


{{#contentFor "bottomRight"}}
    <div class="music-wave-holder" style="position: relative; height: 83px;">
        <img class="music-wave-off" style="position: absolute; top: 0; left: 0; height: 83px; pointer-events: none;">

        <div class="music-wave-on-holder" style="position: absolute; top: 0px; left: 0; overflow: hidden; pointer-events: none;">
            <img class="music-wave-on" style="height: 83px; pointer-events: none;">
        </div>


        <div class="music-wave-marker" disabled="disabled" style="width: 1px; background: orange; height: 83px; z-index: 10; position: absolute; bottom: 0px; left: 0px; pointer-events: none;"></div>

        <div class="music-track-length" style="background: #e0e0e0; color: white; position: absolute; bottom: 0px; right: 0px;">00:00</div>
    </div>
{{/contentFor}}

    <link rel="stylesheet" href="http://code.jquery.com/ui/1.10.1/themes/base/jquery-ui.css" />
    <script type="text/javascript" src="http://ajax.microsoft.com/ajax/jquery.ui/1.8.6/jquery-ui.min.js"></script>


    <script src="/javascripts/soundmanagerv297a-20121104/script/soundmanager2.js"></script>
    <script src="/javascripts/underscore-min.js"></script>
    <script src="/javascripts/jquery.hotkeys.js"></script>
    <script src="/javascripts/player.js"></script>
    <script src="/javascripts/ratings.js"></script>
    <script src="/javascripts/jquery.mousehold.js"></script>
